package stx.runloop.pack;

class Anonymous extends TaskBase{
  var on_perform  : Void->Void;
  var on_cancel   : Void->Void;
  public function new(on_perform,on_cancel) {
    this.on_perform = on_perform;
    this.on_cancel  = on_cancel;
    super(false);
  }
  override public function doPerform(){
    this.on_perform();
  }
  //override public function perform(){
    //super.perform();
    //.__.log().trace(this.state);
 // }
  override public function doCancel(){
    this.on_cancel();
  }
  override public function cancel(){
    super.cancel();
    //.__.log().trace(this.state);
  }
}